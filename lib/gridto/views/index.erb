<div class="row-fluid">
  <div class='span3 hidden-phone'></div>
  <div class='span6' style="display: block;">

    <% @publisher.publisher_categories.each_with_index do |category, i| %>
        <a href="./category/<%= category.slug %>" class="category">
          <img src="/images/<%= category.filename %>">
        </a>

        <% if i %2 == 1 %>
            </div>
            <div class='span3 hidden-phone'></div>
            </div>

            <br/>

            <div class="row-fluid">
              <div class='span3 hidden-phone'></div>
              <div class='span6' style="display: block;">
        <% end %>
    <% end %>
    </div>
    <div class='span3 hidden-phone'></div>
    </div>

    <br/>

