
<div id="maincontent">
  <h1><%= t("user.top_users") %></h1>
    <ul>
      <% @top_users.each do |user| %>
        <li>
            <%= link_to user.username, user_path(user) %>
        </li>
      <% end %>
    </ul>

    <h1><%= t("place.top_places") %></h1>

    <ul>
      <% @top_places.each do |place| %>
        <li>
            <%= link_to place.name, place_path(place) %>
        </li>
      <% end %>
      </ul>
</div>

