<!-- CSS for Theme Styles - Double to prevent flickering on change -->
<!-- <link href="/stylesheets/style.css" rel="stylesheet" media="screen" type="text/css" /> -->
<%= stylesheet_link_tag "application.css" %>
<%= stylesheet_link_tag "jquery.lightbox.css" %>
<%= stylesheet_link_tag "jquery.qtip.css" %>
<%= stylesheet_link_tag "jquery.ui.all.css" %>
<%= stylesheet_link_tag "jquery.ui.css" %>

<!-- Social widgets -->
<script charset="utf-8" src="//platform.twitter.com/widgets.js" type="text/javascript"></script>

<!-- jQuery thats loaded before document ready to prevent flickering - Rest are found at the bottom -->
<%= javascript_include_tag "jquery-1.4.1.min.js" %>
<%= javascript_include_tag "jquery.jeditable.js" %>
<%= javascript_include_tag "jquery.lightbox-0.5.min.js" %>
<%= javascript_include_tag "jquery.qtip.js" %>
<%= javascript_include_tag "jquery.cookie.js" %>
<%= javascript_include_tag "jquery.ui.widget.js" %>
<%= javascript_include_tag "jquery.ui.position.js" %>
<%= javascript_include_tag "jquery.ui.autocomplete.js" %>
<%= javascript_include_tag "jquery.ui.autocomplete.html.js" %>
<%= javascript_include_tag "jquery.optionTree.js" %>


<%= yield :javascript_includes %>