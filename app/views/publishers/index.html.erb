<h1>Listing publishers</h1>

<table>
  <tr>
    <th>Username</th>
    <th>Categories</th>
    <th>Css</th>
    <th>Footerpng</th>
    <th>Wellpng</th>
    <th>Liquid</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

  <% @publishers.each do |publisher| %>
      <tr>
        <td><%= publisher.username %></td>
        <td><%= publisher.categories %></td>
        <td><%= publisher.css %></td>
        <td><%= publisher.footerpng %></td>
        <td><%= publisher.wellpng %></td>
        <td><%= publisher.liquid %></td>
        <td><%= link_to 'Show', publisher %></td>
        <td><%= link_to 'Edit', edit_publisher_path(publisher) %></td>
        <td><%= link_to 'Destroy', publisher, confirm: 'Are you sure?', method: :delete %></td>
      </tr>
  <% end %>
</table>

<br/>

<%= link_to 'New Publisher', new_publisher_path %>
