<div class="grid_24">

    <div class="box_top">

        <h2 class="icon list"><%= t "place.places" %></h2>

        <h3 class="right"><a href="#contents"><%= t('basic.top') %></a></h3>
    </div>

    <div class="box_content padding">

    <table class="sorting">
        <% @places.each do |place|%>
            <% content_tag_for :tr, token do %>
              <td>place.name</td>
            <% end %>
        <% end %>
    </table>

    </div>
</div>



